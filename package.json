  "bugs": {
    "url": "https://github.com/rickzx98/fluid-chains/issues"
  },
  "homepage": "https://github.com/rickzx98/fluid-chains#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "expect": "^1.20.2",
    "isparta": "^4.0.0",
    "mocha": "^3.4.1",
    "sinon": "^2.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "lodash": "^4.17.4",
    "object-sizeof": "^1.2.0"
